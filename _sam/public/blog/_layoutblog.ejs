<!DOCTYPE html>
<html id="top">
   
<% include ../_includes/head %>    
    
    
<% var base = '';
   for (i=0;i<current.path.length-1;i++){ base += '../' } %>
<body>
 <div class="section-bars"> <a id="bars" href="#"><span></span></a></div>  
     <% include ../_includes/aside %> 
    
    <div class="wrapper">
      
        
        
<% if(typeof published==='undefined' || published!==false) { %>

  <main class="main">

      <article class="article">
          <h1><%- title %></h1>
          <div class="article-meta">
              <% if(author) { %>
                <a href="http://twitter.com/<%= authors[author].twitter %>"><%= authors[author].name %></a>
              <% } %>
              <time class="time"> on <%- date %></time>
          </div>
          <%- yield %>
          
          
        
          
          
      </article>
      
       <% include ../_includes/share %>  

      <% include ../_includes/comments %>

  </main>

  <nav class="subnav">

    <%- partial("../_includes/article-nav") %>

  </nav>

<% } else { %>

  <% include ../404 %>

<% } %>
        
        
     
<% include ../_includes/footer %>
      
    </div>

 
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
 <script type="text/javascript" src="<%= base %>js/index.js"></script>     

<% if(service.prism) { %>
  <script src="<%= base %>js/prism.js"></script>
<% } %>
    
   <% include ../_includes/analytics %>  
    
</body>
</html>      